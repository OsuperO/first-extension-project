{
  "name": "My first Extension!",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "statics/js/background.js"
  },
  
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "css": ["static/css/content.css"],
        "js": ["static/js/content.js"],
        "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "activeTab", 
    "scripting",
    "sidePanel"
  ],
  "host_permissions":[],
  "web_accessible_resources": [
    {
      "resources": [ "/images/app.png" ],
      "matches": ["<all_urls>"]
    },
    {
      "resources": [ "insert.js" ],
      "matches": ["<all_urls>"]
    }
  ],
  "side_panel": {
    "default_path": "sidePanel.html"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/brain.png",
      "32": "/images/brain.png",
      "48": "/images/brain.png",
      "128": "/images/brain.png"
    }
  },
  "icons": {
    "16": "/images/brain.png",
    "32": "/images/brain.png",
    "48": "/images/brain.png",
    "128": "/images/brain.png"
  }
}